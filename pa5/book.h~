#include <string>
using namespace std;
#ifndef BOOK_H
#define BOOK_H

class Book{
private:
    string title, author;
    float rating;
    long ISBN;
    int pages;

public:
    Book();
    Book(string t, string a, float r, long i, int p);
    ~Book();
    void printBook();
    long getKey();
    string getTitle();
};
#endif
